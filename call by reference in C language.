#include<stdio.h>
#include<conio.h>
void swap(int *,int *);
int main(){
	int a,b;
	printf("\n Enter any two number");
	scanf("%d%d",&a,&b);
	printf("\n Values of variable before swapping = ");
	printf("\n a=%d,b=%d",a,b);
	swap(&a,&b);
	printf("\n Values of variable after swapping = ");
	printf("\n a=%d,b=%d",a,b);
	getch();
}
void swap(int *a,int *b){
	int c;
	c=*a;
	*a=*b;
	*b=c;
	printf("\n Values of variable after swap = ");
	printf("\n a=%d,b=%d",*a,*b);
}
