#include <iostream>

using namespace std;
int s;

void vol(int);
void vol(int);
void vol(int,int);
void vol(int , int);
    
void vol(int r){
        s = (4*3.14159*r*r*r)/3;
        cout<<"volume of sphere = "<<s;
    }
void vol(int a,int s){
        s=a*a*a;
        cout<<"\n volume of cube = "<<s;
    }
void vol(int r,int h,int s){
        s=3.14159*r*r*h;
        cout<<"\n Volume of cylinder = "<<s;
    }
void vol (int r,int h,int a,int x){
