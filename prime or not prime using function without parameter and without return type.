#include <stdio.h>
#include<conio.h>
void prime();
void main()
{
	prime();
	getch();
}
void prime()
{
	int flag=0,n,i;
	printf("\nEnter the number: ");
	scanf("%d",&n);
	for(i=2;i<n;i++)
	{
		if(n%i==0){
			flag=1;
			break;
		}
	}
	if(flag==0)
	printf("\n%d is prime number",n);
	else
	printf("\n%d is not prime number",n);
}
