#include <stdio.h>
#include <conio.h>
void main(){
    int per;
    printf("given percentage is : ");
    scanf("%d",&per);
    if (90<=per && 100>per){
        printf("Grade of fstudent is A+");
    }
    if (80<=per && 90>per){
        printf("Grade of fstudent is A");
    }
     if (70<=per && 80>per){
        printf("Grade of fstudent is B+");
    }
     if (60<=per && 70>per){
        printf("Grade of fstudent is B");
    }
     if (50<=per && 60>per){
        printf("Grade of fstudent is C+");
    }
     if (40<=per && 50>per){
        printf("Grade of fstudent is C");
    }
     if (40>per){
        printf("Welcome Back");
    }
    getch();
}
