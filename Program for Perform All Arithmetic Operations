#include <iostream>
using namespace std;
int main()
{
int a,b,add,sub,mult,div,per;
cout <<"Enter the first number";
cin>>a;
cout<<"Enter the second number";
cin>>b;
 {
 	add = a+b;
 	cout<<"add number is:  "<<add;}
 	{
 		sub=a-b;
 		cout<<"\n sub number is: "<<sub;
    }
    {
    	mult =a*b;
    	cout <<"\n mult number is: "<<mult;
	} 
	{
		div = a/b;
		cout <<"\n div number is: "<<div;
	}
	{
		per = a%b;
		cout <<"\n per number is: %"<<per;
	}
	return 0;
}
