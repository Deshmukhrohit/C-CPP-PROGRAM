#include<iostream>
#include<conio.h>
using namespace std;
class M
{
	public:
		void showM()
		{
			cout<<"\nshow function of class-N";
		}
};
class N
{
	public:
		void showN()
		{
			cout<<"\nShow function of class - N";
		}
};
class p:public M,public N
{
	public:
		void showp()
		{
			cout<<"\nshow function of class - P";
		}
};
int main()
{
	p x;
	x.showM();x.showN();
	x.showp();
	getch();return 0;
}
