#include<iostream>
#include<conio.h>
using namespace std;
class test{
	int a;
	float b;
	public:
		test()
    {
			a=10;
			b=4.5;
		}
		void input()
    {
			cout<<"\nEnter any integer and float value";
			cin>>a>>b;
		}
		void output()
    {
			cout<<"\nValue of integer variable = "<<a;
			cout<<"\nValue of Float variable = "<<b;
		}
		test(int m,int n)
    {
			a=m;
			b=n;
		}
};
int main()
{
	test t1(23,5.6);
	t1.output();
	test t2(100,8.9);
	t2.output();
	test t3;
	t3.output();
	getch();
	return 0;
}
