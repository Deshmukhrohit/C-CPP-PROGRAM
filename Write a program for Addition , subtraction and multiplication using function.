#include <iostream>
using namespace std;
void sum (int a,int b,int c);
void sum (int a,int b,int c)
{   
    cout<<"\nEnter any two number : ";
    cin>>a>>b;
    c=a+b;
    cout<<"\nsum =  "<<c;
}
void sub (int a,int b,int c);
void sub (int a,int b,int c)
{   
    cout<<"\nEnter any two number : ";
    cin>>a>>b;
    c=a-b;
    cout<<"\nsub =  "<<c;
}
void mult (int a,int b,int c);
void mult (int a,int b,int c)
{   
    cout<<"\nEnter any two number : ";
    cin>>a>>b;
    c=a*b;
    cout<<"\nmult =  "<<c;
}
int main()
{   int a,b,c;
    sum(a,b,c);
    sub(a,b,c);
    mult(a,b,c);
   
    return 0;
}
