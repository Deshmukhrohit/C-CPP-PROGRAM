#include <stdio.h>
void reverse(int);
int main()
{   int n;
    printf("\nEnter the number: ");
    scanf("%d",&n);
    reverse(n);
     return 0;
}
void reverse(int a)
{
    int r,b=0;
    while(a!=0)
    {
        r=a%10;
        a=a/10;
        b=b*10+r;
    }
    printf("\nReverse no.=%d",b);
}
